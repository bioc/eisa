body {
	text-align:center;
	font-family:"Trebuchet ms",Arial,Helvetica,sans-serif;
	font-size:.9em;
	background:#f8f8f8;
	color:#000;
        text-align: left;
	margin: 0px 20px 0px 20px;
}
h1 {
    color: #fff;
    margin: 0px 0px 10px 0px;
    padding: 7px 0 7px 15px;
    text-shadow: 0px 1px 2px #000;
    background: #aeaeae url(images/headerbg.png) repeat-x;
    border-bottom: 1px solid #1c477f;
    font-size: large;
}
h2 {
    color: #fff;
    margin: 10px 0px 10px 0px;
    padding: 7px 0 7px 15px;
    text-shadow: 0px 1px 2px #000;
    background: #aeaeae url(images/headerbg.png) repeat-x;
    border-bottom: 1px solid #1c477f;
    font-size: 1em;
    text-align: left;
}
h3 {
    font-size: 1em;
    text-align: left;
}
h4 {
    font-size: 0.8em;
    text-align: left;
}
td {
    padding-right: 2em;
    padding-left: 1em;
    border-left: 1px solid #AAA;
}
th { 
    padding-right: 2em;
    padding-left: 1em;
    padding-bottom: 5px;
    border-left: 1px solid #AAA;
    border-bottom: 1px solid #AAA;
}
a { color: #00f; text-decoration: none }
a:visited { color: #00c }
a:hover { color: #00f; text-decoration: underline }
h1 a, h1 a:visited, h1 a:hover { color: #fff; text-decoration: none }
h2 a, h2 a:visited, h2 a:hover { color: #000; text-decoration: none }
h3 a, h3 a:visited, h3 a:hover { color: #000; text-decoration: none }
ul.menu {
  font-size: 0.8em;
  list-style-type: none;
  padding: 0; margin: 0;
}
ul.menu li {
  display: inline;
}
ul.menu li:before {
  content: "\00BB \0020";
}
ul.menu li.first:before {
  content: " ";
}
ul.menu2 {
  font-size: 0.8em;
  list-style-type: none;
  padding: 0; margin: 0;
}
ul.menu2 li {
  display: inline;
}
ul.menu2 li:before {
  content: "\00AB \0020";
}
ul.menu2 li.first:before {
  content: " ";
}
span.hideshow {
  float: right;
  font-size: 0.8em;
  color: #FFF;
  margin: 5px 10px;
  clear: right;  
  position: relative;
  width: 300px;
  text-align: right;
}
span.hideshow a, div.hideshow a:visited { color: #FFF }
div.logo {
  float: right;   
}
p.note {
  color: darkgrey;
  font-weight: bold;
  font-size:0.8em;
  margin: 0px 0px 10px 50px;
}

