a span {
	display: none;
}

a:hover {
	position: relative;
}

a:hover span {
	display: block;
   	position: absolute;
	width:200px;
	padding:4px;
	border:#222 2px solid;
	background:#666;
	color:#fff;
	-moz-border-radius:8px;
	border-radius:8px;
	z-index:11;
	font-size:small;
}

a:hover span {
	top: 20px; left: 20px;
}

tr.under {
  color: green;
}
tr.under a {
  color: green;
}
tr.over {
  color: red;
}
tr.over a {
  color: red;
}
tr.even {
  background-color: #EEE;
}
tr.head {
  background-color: #CCC;
}

/* START OF CSS FOR MAP */
dl#mapbp.map.on {
	position:relative;
	text-align:left;
	margin:0px;
	padding:0px;
	width:/*mapbpwidth*/px;
	height:/*mapbpheight*/px;
	background:url(go-BP-/*no*/.png) no-repeat;
}
dl#mapcc.map.on {
	position:relative;
	text-align:left;
	margin:0px;
	padding:0px;
	width:/*mapccwidth*/px;
	height:/*mapccheight*/px;
	background:url(go-CC-/*no*/.png) no-repeat;
}
dl#mapmf.map.on {
	position:relative;
	text-align:left;
	margin:0px;
	padding:0px;
	width:/*mapmfwidth*/px;
	height:/*mapmfheight*/px;
	background:url(go-MF-/*no*/.png) no-repeat;
}
dl.map.on dt {
	list-style:none;
	display:inline;
}
dl.map.on dd {
	position:absolute;
	left:-9999px;
	width:400px;
	padding:4px;
	border:#222 2px solid;
	background:#666;
	color:#fff;
	-moz-border-radius:8px;
	border-radius:8px;
	z-index:11;
	font-size:0.8em;
}
dl.map.on a.location {
	display:block;
	position:absolute;
	text-indent:-9999px;
	outline:none;
	z-index:10;
	text-decoration:none;
}
dl#mapbp.map.on a.location {
	width:55px;
	height:16px;
}
dl#mapcc.map.on a.location {
	width:55px;
	height:16px;
}
dl#mapmf.map.on a.location {
	width:55px;
	height:16px;
}
dl.map.on a.location:hover {
	background:url(images/point-hover2.png) no-repeat 0px 0px;
}
dl.map.on dd a {
	text-decoration:none;
	color:#000;
}

dl#mapcloseupBP.map.on {
	position:relative;
	text-align:left;
	margin:0px;
	padding:0px;
	width:1200px;
	height:460px;
	background:url(closeup-BP-123.png) no-repeat;
}
dl#mapcloseupCC.map.on {
	position:relative;
	text-align:left;
	margin:0px;
	padding:0px;
	width:1200px;
	height:460px;
	background:url(closeup-CC-123.png) no-repeat;
}
dl#mapcloseupMF.map.on {
	position:relative;
	text-align:left;
	margin:0px;
	padding:0px;
	width:1200px;
	height:460px;
	background:url(closeup-MF-123.png) no-repeat;
}
dl#mapcloseupBP.map.on a.location {
	width:105px;
	height:19px;
}
dl#mapcloseupCC.map.on a.location {
	width:105px;
	height:19px;
}
dl#mapcloseupMF.map.on a.location {
	width:105px;
	height:19px;
}

dl#mapover.map.on {
	position:relative;
	text-align:left;
	margin:0px;
	padding:0px;
	width:1200px;
	height:600px;
	background:url(overlap-/*gth*/.png) no-repeat;
}
dl#mapover.map.on a.location {
	width:32px;
	height:15px;
}


